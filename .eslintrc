{
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off",
    "class-property/class-property-semicolon": 1,
    "semi": [
      2,
      "never"
    ]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "class-property"
  ],
  "extends": "eslint:recommended"
}